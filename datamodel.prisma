type User {
  id: ID! @unique
  userType: Int
  userEmail: String @unique
  userPassword: String
  confirmHash: String
  forgotHash: String
  userConfirmed: Int
  userDeleted: Boolean @default(value: "false")
  userMeta: [UserMeta!]!
}

type UserMeta {
  id: ID! @unique
  user: User
  metaType: String
  metaName: String!
  metaValue: String!
  metaDeleted: Boolean @default(value: "false")
}

