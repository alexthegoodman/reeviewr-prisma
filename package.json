  "name": "your-app",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    "IE 11"
  ],
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "nf start web=1,worker=1",
    "build:jsonschema": "node scripts/json-schema-types.js",
    "build:graphql": "prisma generate; yarn gen:gql",
    "build:client": "webpack --config ./webpack/client.config.js",
    "build:server": "webpack --config ./webpack/server.config.js",
    "build:pre": "concurrently 'yarn build:jsonschema' 'yarn build:graphql'",
    "build:app": "concurrently 'yarn build:client' 'yarn build:server'",
    "build": "yarn build:pre && yarn build:app",
    "build:production": "cross-env NODE_ENV=production yarn build",
    "dev:jsonschema": "nodemon -C -w 'modules/**/*.schema.json' ./scripts/json-schema-types.js",
    "dev:graphql": "nodemon -C -w 'modules/**/*.graphql' 'yarn build:graphql'",
    "dev:server:webpack": "webpack --config webpack/server.config.js --watch",
    "dev:server:nodemon": "nodemon --watch dist/server.js -- --inspect=9229 dist/server.js",
    "dev:worker:nodemon": "nodemon --watch 'dist/scripts/{job-worker.js,*processor.js}' -- --inspect=9230 dist/scripts/job-worker.js",
    "dev:client": "webpack-dev-server --config webpack/client.config.js",
    "dev": "yarn build:pre && concurrently -p name -c \"yellow,magenta,green,blue,gray\" -n \"webpack-server,nodemon-server,webpack-client,graphql-types,json-schema,workers\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\" \"yarn dev:client\" \"yarn dev:graphql\" \"yarn dev:jsonschema\" \"yarn dev:worker:nodemon\"",
    "dev:storybook": "start-storybook -p 9001 -c .storybook --ci",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json './modules/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix -c ./tslint.json -p ./tsconfig.json './modules/**/*.{ts,tsx}'",
    "fmt:prettier": "prettier --write 'modules/**/*.{ts,tsx,js,jsx}'",
    "fmt": "yarn fmt:prettier; yarn lint:fix",
    "pretest": "yarn lint",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:unit:ci": "cross-env NODE_ENV=test jest --maxWorkers 8",
    "test:unit:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "test:integration": "cypress run",
    "docker:up": "docker-compose up -d",
    "kill-list": "lsof -i :9229; lsof -i :9230",
    "gen:ui": "bash/gen_ui_component.sh",
    "gen:page": "bash/gen_page_component.sh",
    "gen:data": "bash/gen_data_component.sh",
    "gen:layout": "bash/gen_layout_component.sh",
    "gen:gql": "graphql-codegen --config codegen.yml",
    "heroku:deploy": "git push heroku master",
    "prisma:deploy:local": "NODE_ENV=http://localhost:4466 prisma deploy",
    "prisma:deploy:dev": "NODE_ENV=https://us1.prisma.sh/alex-woodman/reeviewr-prisma/dev prisma deploy",
    "prisma:deploy:prod": "NODE_ENV=https://us1.prisma.sh/alex-woodman/reeviewr-prisma/prod prisma deploy"
  },
